{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "https://example.com/schemas/get_voltage.json",
  "type": "object",
  "title": "Get_All_Voltages",
  "description": "Gets all voltages applied to a target",
  "additionalProperties": false,
  "properties": {
    "getters": {
      "type": "array",
      "description": "Instruments to read from.",
      "items": {
        "$ref": "../lib/instrument_target.json#/definitions/InstrumentTarget"
      }
    }
  },
  "returns": {
    "type": "array",
    "description": "List of MeasurementResponse with a numeric value in V.",
    "items": {
      "allOf": [
        {
          "$ref": "../lib/measurement_response.json#/definitions/MeasurementResponse"
        },
        {
          "description": "Annotates the concrete type of the MeasurementResponse value",
          "x-valueType": "number"
        }
      ]
    }
  }
}
